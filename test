local function XAUSAM_fake_script() -- PressButton.PressHandler 
	local script = Instance.new('LocalScript', PressButton)

	local PressButton = script.Parent
	local TextBox = script.Parent.Parent.TextBox
	
	function Notify(title, desc, duration)
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = title,
			Text = desc,
			Duration = duration,
		})
	end
	
	PressButton.MouseButton1Down:Connect(function()
		if TextBox.Text == "" then
			local PlaceHolder = true
		else
			Notify("This is for you!", TextBox.Text, 5)
			wait(0.05)
			TextBox.Text = ""
		end
	end)
end
coroutine.wrap(XAUSAM_fake_script)()
local function POPTP_fake_script() -- SliderButton.SliderHandler 
	local script = Instance.new('LocalScript', SliderButton)

	--{0, 290},{0, 33} full size
	
	local SliderButton = script.Parent
	local SliderInner = script.Parent.SliderInner
	local SliderValue = script.Parent.Parent.SliderValue
	
	-----Values-----
	minvalue = minvalue or 0
	maxvalue = maxvalue or 100
	
	
	-----Callback-----
	callback = callback or function() end
	
	
	-----Variables-----
	local mouse = game.Players.LocalPlayer:GetMouse()
	local uis = game:GetService("UserInputService")
	local Value;
	
	
	
	
	-----Main Code-----
	
	SliderButton.MouseButton1Down:Connect(function()
		Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 290) * SliderInner.AbsoluteSize.X) + tonumber(minvalue)) or 0
		pcall(function()
			callback(Value)
		end)
		SliderInner.Size = UDim2.new(0, math.clamp(mouse.X - SliderInner.AbsolutePosition.X, 0, 290), 0, 33)
		moveconnection = mouse.Move:Connect(function()
			SliderValue.Text = Value
			Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 290) * SliderInner.AbsoluteSize.X) + tonumber(minvalue))
			pcall(function()
				callback(Value)
			end)
			SliderInner.Size = UDim2.new(0, math.clamp(mouse.X - SliderInner.AbsolutePosition.X, 0, 290), 0, 33)
		end)
		releaseconnection = uis.InputEnded:Connect(function(Mouse)
			if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
				Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 290) * SliderInner.AbsoluteSize.X) + tonumber(minvalue))
				pcall(function()
					callback(Value)
				end)
				SliderInner.Size = UDim2.new(0, math.clamp(mouse.X - SliderInner.AbsolutePosition.X, 0, 290), 0, 33)
				moveconnection:Disconnect()
				releaseconnection:Disconnect()
			end
		end)
	end)
end
coroutine.wrap(POPTP_fake_script)()
local function PTAA_fake_script() -- DropDownOpener.DropDownOpenerHandler 
	local script = Instance.new('LocalScript', DropDownOpener)

	local DropDownOpener = script.Parent
	local DropDownOpenerImage = script.Parent.DropDownOpenerImage
	local DropDownSelectionFrame = script.Parent.Parent.DropDownSelectionFrame
	local TestPage = script.Parent.Parent.Parent.Parent.TestPage
	
	local TweenService = game:GetService("TweenService")
	
	DropDownOpener.MouseButton1Down:Connect(function()
		if DropDownOpenerImage.Rotation == 90 then
			TweenService:Create(DropDownOpenerImage, TweenInfo.new(0.8), {Rotation = -90}, Enum.EasingStyle.Quad):Play()
			DropDownSelectionFrame.ScrollBarImageTransparency = 0
			DropDownSelectionFrame:TweenSize(UDim2.new(0, 243,0, 57), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.8)
			TweenService:Create(TestPage, TweenInfo.new(0.8), {CanvasSize = TestPage.CanvasSize + UDim2.new(0,0,0.18,0)}, Enum.EasingStyle.Quad):Play()
			TweenService:Create(TestPage, TweenInfo.new(0.8), {CanvasPosition = TestPage.CanvasPosition + Vector2.new(0, 67.1)}, Enum.EasingStyle.Quad):Play()
		elseif DropDownOpenerImage.Rotation == -90 then
			TweenService:Create(DropDownOpenerImage, TweenInfo.new(0.8), {Rotation = 90}, Enum.EasingStyle.Quad):Play()
			DropDownSelectionFrame.ScrollBarImageTransparency = 1
			DropDownSelectionFrame:TweenSize(UDim2.new(0, 243,0, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.8)
			TweenService:Create(TestPage, TweenInfo.new(0.8), {CanvasSize = TestPage.CanvasSize - UDim2.new(0,0,0.18,0)}, Enum.EasingStyle.Quad):Play()
		end
	end)
	
end
coroutine.wrap(PTAA_fake_script)()
local function FQHEOP_fake_script() -- HotDogButton.DropDownSelectionHandler 
	local script = Instance.new('LocalScript', HotDogButton)

	local SelectButton = script.Parent
	local DropDownOpener = script.Parent
	local DropDownOpenerImage = script.Parent.Parent.Parent.DropDownOpener.DropDownOpenerImage
	local DropDownSelectionFrame = script.Parent.Parent
	local TestPage = script.Parent.Parent.Parent.Parent
	
	local SelectedLabel = script.Parent.Parent.Parent.InnerFrame.SelectedLabel
	
	local TweenService = game:GetService("TweenService")
	
	
	
	
	SelectButton.MouseButton1Down:Connect(function()
		TweenService:Create(DropDownOpenerImage, TweenInfo.new(0.8), {Rotation = 90}, Enum.EasingStyle.Quad):Play()
		DropDownSelectionFrame.ScrollBarImageTransparency = 1
		DropDownSelectionFrame:TweenSize(UDim2.new(0, 243,0, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.8)
		TweenService:Create(TestPage, TweenInfo.new(0.8), {CanvasSize = TestPage.CanvasSize - UDim2.new(0,0,0.18,0)}, Enum.EasingStyle.Quad):Play()
		print("pls work")
		SelectedLabel.Text = SelectButton.Text
	end)
end
coroutine.wrap(FQHEOP_fake_script)()
