local function UJXGV_fake_script() -- ToggleButton.ToggleHandler 
	local script = Instance.new('LocalScript', ToggleButton)

	local ToggleButton = script.Parent
	
	local TweenService = game:GetService("TweenService")
	
	ToggleButton.MouseButton1Down:Connect(function()
		if ToggleButton.BackgroundColor3 == Color3.fromRGB(0, 136, 255) then -- off
			TweenService:Create(ToggleButton, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(18, 18, 18)}):Play()
		else --on
			TweenService:Create(ToggleButton, TweenInfo.new(0.42), {BackgroundColor3 = Color3.fromRGB(0, 136, 255)}):Play()
			
			--script
			
			
		end
	end)
end
