local HoldHub = {}
 
function HoldHub:CreateWindow(title, gamename)
	local HoldHubLibrary = Instance.new("ScreenGui")
	local MainFrame = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local DraggingFrame = Instance.new("Frame")
	local HubTitle = Instance.new("TextLabel")
	local GameName = Instance.new("TextLabel")
	local MainHolder = Instance.new("Frame")
 
	local UICorner_2 = Instance.new("UICorner")
	local TabFrame = Instance.new("ScrollingFrame")
	local Frame = Instance.new("Frame")
	local UICorner_3 = Instance.new("UICorner")
	local UIGridLayout = Instance.new("UIGridLayout")
    
    local Pages = Instance.new("Folder")
 
	HoldHubLibrary.Name = "Hold-Hub-Library"
	HoldHubLibrary.Parent = game.CoreGui
	HoldHubLibrary.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
 
	MainFrame.Name = "MainFrame"
	MainFrame.Parent = HoldHubLibrary
	MainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
	MainFrame.ClipsDescendants = true
	MainFrame.Position = UDim2.new(0.535854459, 0, 0.555282533, 0)
	MainFrame.Size = UDim2.new(0, 487, 0, 337)
 
	UICorner.CornerRadius = UDim.new(0, 5)
	UICorner.Parent = MainFrame
 
	DraggingFrame.Name = "DraggingFrame"
	DraggingFrame.Parent = MainFrame
	DraggingFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	DraggingFrame.BackgroundTransparency = 1.000
	DraggingFrame.BorderSizePixel = 0
	DraggingFrame.Size = UDim2.new(0, 487, 0, 38)
 
	HubTitle.Name = "HubTitle"
	HubTitle.Parent = MainFrame
	HubTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	HubTitle.BackgroundTransparency = 1.000
	HubTitle.Position = UDim2.new(0.0238931961, 0, 0, 0)
	HubTitle.Size = UDim2.new(0, 104, 0, 24)
	HubTitle.Font = Enum.Font.SourceSans
	HubTitle.Text = title or "Hold-Library"
	HubTitle.TextColor3 = Color3.fromRGB(0, 136, 255)
	HubTitle.TextSize = 22.000
	HubTitle.TextStrokeColor3 = Color3.fromRGB(52, 52, 52)
	HubTitle.TextStrokeTransparency = 0.700
	HubTitle.TextXAlignment = Enum.TextXAlignment.Left
 
	GameName.Name = "GameName"
	GameName.Parent = MainFrame
	GameName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	GameName.BackgroundTransparency = 1.000
	GameName.BorderSizePixel = 0
	GameName.Position = UDim2.new(0.0225872695, 0, 0.0709677711, 0)
	GameName.Size = UDim2.new(0, 113, 0, 16)
	GameName.Font = Enum.Font.SourceSansLight
	GameName.Text = gamename or "Not a valid string."
	GameName.TextColor3 = Color3.fromRGB(255, 255, 255)
	GameName.TextSize = 16.000
	GameName.TextXAlignment = Enum.TextXAlignment.Left
 
	MainHolder.Name = "MainHolder"
	MainHolder.Parent = MainFrame
	MainHolder.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
	MainHolder.Position = UDim2.new(0, 0, 0.122580692, 0)
	MainHolder.Size = UDim2.new(0, 487, 0, 295)
 
	UICorner_2.Parent = MainHolder
 
	TabFrame.Name = "TabFrame"
	TabFrame.Parent = MainHolder
	TabFrame.Active = true
	TabFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TabFrame.BackgroundTransparency = 1.000
	TabFrame.BorderSizePixel = 0
	TabFrame.Position = UDim2.new(0.0410677604, 0, 0.0441176593, 0)
	TabFrame.Size = UDim2.new(0, 140, 0, 274)
	TabFrame.CanvasSize = UDim2.new(0, 0, 0.899999976, 0)
	TabFrame.ScrollBarThickness = 2
 
	UIGridLayout.Parent = TabFrame
	UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
	UIGridLayout.CellPadding = UDim2.new(0, 5, 0, 0)
	UIGridLayout.CellSize = UDim2.new(0, 133, 0, 50)

    Pages.Name = "Pages"
    Pages.Parent = MainHolder

    local UIS = game:GetService('UserInputService')
	local dragToggle = nil
	local dragSpeed = 0.12
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(MainFrame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	DraggingFrame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = MainFrame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)

    --^^Dragging
 
	local Window = {}
 
	function Window:CreatePage(pagename, canvassize, mainframe)
 
		local TestPage = Instance.new("ScrollingFrame")
		local UICorner_4 = Instance.new("UICorner")
		local UIGridLayout_2 = Instance.new("UIGridLayout")
        local TabButton = Instance.new("TextButton")
        local Frame = Instance.new("Frame")
        local UICorner_3 = Instance.new("UICorner")
        local UIGridLayout = Instance.new("UIGridLayout")
 
		TestPage.Name = pagename
		TestPage.Parent = Pages
		TestPage.Active = true
		TestPage.BackgroundColor3 = Color3.fromRGB(16, 16, 16)
		TestPage.BorderSizePixel = 0
		TestPage.Position = UDim2.new(0.357000023, 0, 0.0219999664, 0)
		TestPage.Size = UDim2.new(0, 307, 0, 281)
		TestPage.CanvasSize = canvassize or UDim2.new(0, 0, 1, 0)
		TestPage.ScrollBarThickness = 2
 
		UICorner_4.Parent = TestPage
 
		UIGridLayout_2.Parent = TestPage
		UIGridLayout_2.HorizontalAlignment = Enum.HorizontalAlignment.Center
		UIGridLayout_2.SortOrder = Enum.SortOrder.LayoutOrder
		UIGridLayout_2.CellPadding = UDim2.new(0, 5, 0, 4)
		UIGridLayout_2.CellSize = UDim2.new(0, 296, 0, 39)

        TabButton.Parent = TabFrame
        TabButton.BackgroundColor3 = Color3.fromRGB(16, 16, 16)
        TabButton.BackgroundTransparency = 1.000
        TabButton.BorderSizePixel = 0
        TabButton.Position = UDim2.new(0.0501168519, 0, 0.00833825767, 0)
        TabButton.Size = UDim2.new(0, 133, 0, 50)
        TabButton.AutoButtonColor = false
        TabButton.Font = Enum.Font.SourceSansLight
        TabButton.Text = pagename or "Test Page"
        TabButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TabButton.TextSize = 18.000
        TabButton.TextStrokeTransparency = 0.300
        TabButton.TextXAlignment = Enum.TextXAlignment.Left

        Frame.Parent = TextButton
        Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Frame.BackgroundTransparency = mainframe or 0.74
        Frame.Position = UDim2.new(-0.00654131686, 0, 0.779999971, 0)
        Frame.Size = UDim2.new(0, 29, 0, 2)

        UICorner_3.CornerRadius = UDim.new(1, 0)
        UICorner_3.Parent = Frame

        UIGridLayout.Parent = TabFrame
        UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
        UIGridLayout.CellPadding = UDim2.new(0, 5, 0, 0)
        UIGridLayout.CellSize = UDim2.new(0, 133, 0, 50)

        local Page = {}
        
        function Page:AddSection(sectionname) 

            local Section = Instance.new("Frame")
            local SectionName = Instance.new("TextLabel")
            
            Section.Name = "Section"
            Section.Parent = TestPage
            Section.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Section.BackgroundTransparency = 1.000
            Section.BorderSizePixel = 0
            Section.Position = UDim2.new(-0.021172639, 0, 0, 0)
            Section.Size = UDim2.new(0, 308, 0, 39)

            SectionName.Parent = Section
            SectionName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SectionName.BackgroundTransparency = 1.000
            SectionName.BorderSizePixel = 0
            SectionName.Size = UDim2.new(0, 296, 0, 39)
            SectionName.Font = Enum.Font.SourceSansBold
            SectionName.Text = sectionname or "Section"
            SectionName.TextColor3 = Color3.fromRGB(255, 255, 255)
            SectionName.TextSize = 18.000
        end

        funtion Page:AddButton(buttonname, callback)

            local callback = callback or function() end

            local Button = Instance.new("Frame")
            local ButtonClick = Instance.new("TextButton")
            local UICorner_5 = Instance.new("UICorner")

            Button.Name = "Button"
            Button.Parent = TestPage
            Button.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
            Button.BorderSizePixel = 0
            Button.Position = UDim2.new(-0.021172639, 0, 0, 0)
            Button.Size = UDim2.new(0, 308, 0, 39)

            ButtonClick.Name = "ButtonClick"
            ButtonClick.Parent = Button
            ButtonClick.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ButtonClick.BackgroundTransparency = 1.000
            ButtonClick.BorderSizePixel = 0
            ButtonClick.Size = UDim2.new(0, 296, 0, 39)
            ButtonClick.Font = Enum.Font.SourceSans
            ButtonClick.Text = buttonname or "Button"
            ButtonClick.TextColor3 = Color3.fromRGB(255, 255, 255)
            ButtonClick.TextSize = 16.000

            UICorner_5.CornerRadius = UDim.new(0, 6)
            UICorner_5.Parent = Button

            ButtonClick.MouseButton1Down:Connect(function()
                pcall(callback)
            end)
		end
			--[[
            function Page:AddToggle(togglename, callback)
                
                local Toggle1 = Instance.new("Frame")
                local UICorner_6 = Instance.new("UICorner")
                local TextLabel_2 = Instance.new("TextLabel")
                local ToggleButton = Instance.new("TextButton")
                local UICorner_7 = Instance.new("UICorner")

                Toggle1.Name = "Toggle1"
                Toggle1.Parent = TestPage
                Toggle1.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
                Toggle1.BorderSizePixel = 0
                Toggle1.Position = UDim2.new(-0.021172639, 0, 0, 0)
                Toggle1.Size = UDim2.new(0, 308, 0, 39)

                UICorner_6.CornerRadius = UDim.new(0, 6)
                UICorner_6.Parent = Toggle1

                TextLabel_2.Parent = Toggle1
                TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                TextLabel_2.BackgroundTransparency = 1.000
                TextLabel_2.Position = UDim2.new(0.0270270277, 0, 0, 0)
                TextLabel_2.Size = UDim2.new(0, 288, 0, 39)
                TextLabel_2.Font = Enum.Font.SourceSans
                TextLabel_2.Text = togglename or "Toggle"
                TextLabel_2.TextColor3 = Color3.fromRGB(255, 255, 255)
                TextLabel_2.TextSize = 16.000
                TextLabel_2.TextXAlignment = Enum.TextXAlignment.Left

                ToggleButton.Name = "ToggleButton"
                ToggleButton.Parent = Toggle1
                ToggleButton.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
                ToggleButton.Position = UDim2.new(0.879999995, 0, 0.0769999996, 0)
                ToggleButton.Size = UDim2.new(0, 33, 0, 33)
                ToggleButton.AutoButtonColor = false
                ToggleButton.Font = Enum.Font.SourceSans
                ToggleButton.Text = ""
                ToggleButton.TextColor3 = Color3.fromRGB(0, 0, 0)
                ToggleButton.TextSize = 14.000

                UICorner_7.Parent = ToggleButton

        end]]

        return Page

	end
 
	return Window
 
end
 
return HoldHub






